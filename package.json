{
  "name": "stripe-connect-furever",
  "description": "A sample on-demand platform running on top of Stripe Connect embedded components.",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stripe/stripe-connect-furever.git"
  },
  "type": "module",
  "source": "client/index.html",
  "scripts": {
    "build": "parcel build client/index.html --no-cache && yarn typescript",
    "clean": "rimraf dist && rimraf .parcel-cache",
    "dev": "yarn kill-ports && yarn clean && yarn typescript && yarn watch",
    "server": "concurrently --kill-others \"nodemon dist/server/server.js\" \"yarn typescript-watch\"",
    "start": "yarn clean && yarn build && yarn server",
    "typescript": "yarn tsc",
    "typescript-watch": "yarn tsc-watch",
    "watch": "concurrently --kill-others \"parcel watch client/index.html\" \"yarn server\"",
    "kill-ports": "kill-port 3000"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@mui/system": "^5.12.3",
    "@stripe/connect-js": "^1.0.6",
    "@stripe/react-connect-js": "^1.2.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "debug": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-session": "^1.17.3",
    "http-auth": "^4.2.0",
    "http-auth-connect": "^1.0.6",
    "mongoose": "^5.11.9",
    "morgan": "^1.9.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.11.1",
    "stripe": "11.5.0-beta.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express-session": "^1.17.7",
    "@types/http-auth": "^4.1.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "20.1.0",
    "@types/passport": "^1.0.12",
    "@types/passport-local": "^1.0.35",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "concurrently": "^8.0.1",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "parcel": "^2.8.3",
    "prettier": "2.5.1",
    "process": "^0.11.10"
  },
  "engines": {
    "node": ">=14.20.1"
  },
  "targets": {
    "default": {
      "engines": {
        "browsers": ">= 50%"
      }
    }
  }
}
